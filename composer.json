{
    "name": "checkoutweb/sdk",
    "type": "library",
    "description": "Checkoutweb pack sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "checkoutweb\\sdk\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "checkoutweb\\sdk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers":[
                "checkoutweb\\sdk\\PacoteTesteBaseServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Jordan XP",
            "email": "jordan@xpdeal.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.1|^8.0"
    }
}
